[Unit]
Description=Initial Flatpaks Seeding

[Service]
ExecStart=/usr/bin/flatpak install --noninteractive --assumeyes --sideload-repo=/system/@flatpak/.ostree/repo flathub org.mozilla.firefox

[Install]
WantedBy=multi-user.target
